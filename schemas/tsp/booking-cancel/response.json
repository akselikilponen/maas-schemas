{
  "$id": "http://maasglobal.com/tsp/bookings-cancel/response.json",
  "description": "Response schema for canceling a booking through a TSP adapter",
  "type": "object",
  "properties": {
    "tspId": {
      "$ref": "/core/booking.json#/definitions/tspId"
    },
    "state": {
      "description": "For normal full cancellation, end state would be CANCELLED. For booking that has multi tickets, if not all of them are cancelled, response state will be RESERVED",
      "enum": ["CANCELLED", "RESERVED"]
    },
    "cost": {
      "$ref": "/core/booking.json#/properties/cost"
    },
    "leg": {
      "$ref": "/core/booking-option.json#/definitions/leg"
    },
    "meta": {
      "$ref": "/core/booking.json#/definitions/meta"
    },
    "terms": {
      "$ref": "/core/booking.json#/definitions/terms"
    },
    "token": {
      "$ref": "/core/booking.json#/definitions/token"
    },
    "tspProduct": {
      "$ref": "/core/booking-option.json#/definitions/tspProduct"
    }
  },
  "required": ["tspId", "state"],
  "additionalProperties": false
}
